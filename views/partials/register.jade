div(ng-controller="RegisterCtrl", class="container")
    div.row
        div.col-sm-6.col-md-4.col-md-offset-4
            h1.text-center.login-title Registration Details
            div.signup-wall
                form(name="registerForm", class="form-signin" sf-schema="schema", sf-form="form", sf-model="model", ng-submit="onSubmit(registerForm)")
            #message
                if message
                    h1.text-center.error-message #{message}